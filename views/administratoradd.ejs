<form action="/adminaddrest" method="post">
        <div class="form-group">
            <label for="exampleInputEmail1">Unesite naziv restorana</label>
            <input type="text" class="form-control" id="" name = "restaurantname" placeholder="Unesite naziv">

        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Izaberite tip restorana:</label>
            <select name="rid" class="form-control" id="exampleSelect1">
                <%for(let i=0;i<restauranttypes.length;i++) {%>
                <option value="<%=restauranttypes[i].restaurant_type_id%>"><%=restauranttypes[i].name%></option>
                <%}%>
            </select>

        </div>
        <div class="form-group">
            <label for="exampleSelect1">Izaberite tip hrane</label>
            <select name="fid" class="form-control" id="exampleSelect1">
                <%for(let i=0;i<foodtypes.length;i++) {%>
                <option value="<%=foodtypes[i].food_id%>"><%=foodtypes[i].food_name%></option>
                <%}%>

            </select>
        </div>
        <div class="form-group">
            <label for="exampleSelect2">Izaberite broj zvjezdica:</label>
            <select name="sid" multiple="" class="form-control" id="exampleSelect2">
                <%for(let i=0;i<stars.length;i++) {%>
                <%=console.info(stars[i].review_id)%>
                <option value="<%=stars[i].review_id%>"><%=stars[i].number%></option>
                <%}%>

            </select>
        </div>
    <div class="form-group">
        <label for="exampleSelect2">Izaberite grad u kojem se nalazi:</label>
        <select name="cid" multiple="" class="form-control" id="exampleSelect2">
            <<%for(let i=0;i<cities.length;i++) {%>
            <option value="<%=cities[i].city_id%>"><%=cities[i].city_name%></option>
            <%}%>

        </select>
    </div>

        <button type="submit" class="btn btn-primary">Submit</button>
</form>





